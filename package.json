{
  "name": "yyd_web_backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:status": "npx prisma migrate status",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed",
    "db:push": "npx prisma db push",
    "docker:dev": "docker-compose up -d",
    "docker:dev:build": "docker-compose up -d --build",
    "docker:dev:down": "docker-compose down",
    "docker:dev:logs": "docker-compose logs -f",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "deploy:dev": "bash scripts/deploy-dev.sh",
    "deploy:prod": "bash scripts/deploy-prod.sh",
    "update:server": "bash scripts/update-server.sh",
    "migration:helper": "bash scripts/migration-helper.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^12.1.0",
    "multer": "^2.0.2",
    "redis": "^5.8.3",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.17.0"
  }
